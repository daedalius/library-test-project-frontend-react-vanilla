{
  "sourceMap": true,
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "lib": ["DOM", "ES2021"],
    "types": ["jest"],
    "paths": {
      "#api": ["src/api"],
      "#api/*": ["src/api/*"],
      "#components": ["src/components"],
      "#components/*": ["src/components/*"],
      "#entities": ["src/entities"],
      "#entities/*": ["src/entities/*"],
      "#utils": ["src/utils"],
      "#utils/*": ["src/utils/*"],
      "#storybook/*": [".storybook/*"],
      "#/*": ["src/*"]
    },
    "esModuleInterop": true
  },
  "exclude": ["cypress/", "src/**/*.component.tests.tsx"]
}
